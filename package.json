{
  "name": "remix-starter-template",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix build",
    "dev": "remix dev --manual -c \"node --inspect server.js\"",
    "start": "cross-env NODE_ENV=production node ./server.js",
    "typecheck": "tsc",
    "lint": "eslint \"app/**/*.+(ts|tsx)\"",
    "lint:fix": "npm run lint -- --fix",
    "prettier": "prettier app --check",
    "prettier:fix": "prettier app --write",
    "format-code": "npm run prettier:fix & npm run lint:fix"
  },
  "dependencies": {
    "@remix-run/css-bundle": "^2.0.1",
    "@remix-run/express": "^2.0.1",
    "@remix-run/node": "^2.0.1",
    "@remix-run/react": "^2.0.1",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "express": "^4.18.2",
    "isbot": "^3.6.8",
    "morgan": "^1.10.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "source-map-support": "^0.5.21"
  },
  "devDependencies": {
    "@remix-run/dev": "^2.0.1",
    "@remix-run/eslint-config": "^2.0.1",
    "@types/compression": "^1.7.4",
    "@types/express": "^4.17.20",
    "@types/morgan": "^1.9.6",
    "@types/react": "^18.2.28",
    "@types/react-dom": "^18.2.13",
    "@types/source-map-support": "^0.5.6",
    "chokidar": "^3.5.3",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-tailwindcss": "^3.13.0",
    "prettier": "3.0.3",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.1.6"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
